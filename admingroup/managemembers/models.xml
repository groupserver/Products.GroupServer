<xf:model id="manage_group_members"
  xmlns:xf="http://www.w3.org/2002/xforms"
  xmlns:tal="http://xml.zope.org/namespaces/tal" 
  tal:define="group_object here/Scripts/get/group_object; 
  division_object here/Scripts/get/division_object">
  <xf:instance>
    <data>
      <submitted>1</submitted>
      
      <moderate_add_userid/>
      <moderate_remove_userid/>
      
      <moderators_add_userid/>
      <moderators_remove_userid/>
      
      <ptn_coach_add_userid 
        tal:content="group_object/ptn_coach_id | nothing"/>
      <ptn_coach_remove_userid />
      
      <group_admin_add_userid/>
      <group_admin_remove_userid/>
      
      <posting_add_userid/>
      <posting_remove_userid/>
      
      <group_remove_userid/>
      
      <groupid tal:content="group_object/getId">group id</groupid>
      <divisionid
        tal:content="division_object/getId">division id</divisionid>
      </data>
    </xf:instance>
  <xf:submission method="urlencoded-post" id="change"
    tal:attributes="action request/URL" />
</xf:model>
