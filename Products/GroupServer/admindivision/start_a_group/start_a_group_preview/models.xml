<xf:model id="start_a_group"
  xmlns:xf="http://www.w3.org/2002/xforms"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  tal:define="site_object here/Scripts/get/division_object">
  <xf:instance>
    <data>
      <submitted>1</submitted>
      <siteId tal:content="site_object/getId"/>
      <groupType 
        tal:content="python:request.get('groupType', '')"/>
      <groupName 
        tal:content="python:request.get('groupName', '')"/>
      <groupId
        tal:content="python:request.get('groupId', '')"/>
      <realLifeGroup
        tal:content="python:request.get('realLifeGroup', '')"/>
      <privacy
        tal:content="python:request.get('privacy', '')"/>
    </data>
  </xf:instance>
  <xf:submission tal:attributes="action request/URL" method="urlencoded-post" id="preview"/>
  <xf:submission tal:attributes="action request/URL" method="urlencoded-post" id="start"/>
</xf:model>
