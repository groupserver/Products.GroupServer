<?xml version="1.0" encoding="utf-8"?>
<feed version="0.3" xmlns="http://purl.org/atom/ns#"
                    xmlns:tal="http://xml.zope.org/namespaces/tal">
  
  <title tal:content="here/GlobalConfiguration/siteName"/>
  <link rel="alternate" type="text/html" tal:attributes="href string:http://${here/GlobalConfiguration/canonicalHost}"/>
  
  <modified tal:content="python:options['results'][0].modification_time.HTML4()">2003-12-13T18:30:02Z</modified>
  <author>
    <name tal:content="here/GlobalConfiguration/supportEmail"></name>
  </author>
  <entry tal:repeat="result python:map(lambda x: x.getObject(), options['results'])">
    <title tal:content="result/dc_title"/>
    <link rel="alternate" type="text/html" tal:attributes="href result/absolute_url"/>
    <id tal:content="string:${here/GlobalConfiguration/canonicalHost}.news.${result/getId}"/>
    <issued tal:content="python:result.dc_valid.HTML4()">2003-12-13T08:29:29-04:00</issued>
    <modified tal:content="python:result.modification_time().HTML4()">2003-12-13T18:30:02Z</modified>
    <content type="application/xhtml+xml" mode="xml">
      <div xmlns="http://www.w3.org/1999/xhtml"
           tal:content="structure result/dc_description"/>
    </content>
  </entry>

</feed>

